// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider               = "prisma-client"
    output                 = "generated"
    runtime                = "nodejs"
    moduleFormat           = "esm"
    generatedFileExtension = "ts"
    importFileExtension    = ""
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}
